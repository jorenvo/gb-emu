{
  "name": "gb-emu",
  "version": "1.0.0",
  "description": "",
  "main": "gb.ts",
  "scripts": {
    "prebuild": "rm -rvf dist/*",
    "build": "mkdir -p $PWD/dist && tsc && cp $PWD/src/*.{html,css} dist/",
    "watch": "npm-watch",
    "test": "mocha -r ts-node/register src/*.spec.ts",
    "env": "env"
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "ts,html,css",
      "quiet": false
    }
  },
  "keywords": [],
  "author": "Joren Van Onder <joren@jvo.sh>",
  "license": "GPL-3.0-or-later",
  "devDependencies": {
    "@types/assert": "^1.5.1",
    "@types/mocha": "^7.0.2",
    "assert": "^2.0.0",
    "mocha": "^8.0.1",
    "npm-watch": "^0.6.0",
    "prettier": "^1.18.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.6"
  }
}
