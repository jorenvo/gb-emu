{
  "name": "gb-emu",
  "version": "1.0.0",
  "description": "",
  "main": "gb.ts",
  "scripts": {
    "prebuild": "rm -rvf dist/*",
    "build": "mkdir -p $PWD/dist && tsc && cp $PWD/src/*.{html,css} dist/",
    "watch": "npm-watch",
    "env": "env"
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "ts,html,css",
      "quiet": false
    }
  },
  "keywords": [],
  "author": "Joren Van Onder <joren@jvo.sh>",
  "license": "GPL-3.0-or-later",
  "devDependencies": {
    "typescript": "^3.9.6",
    "prettier": "^1.18.2",
    "npm-watch": "^0.6.0"
  }
}
